<ul class="nav nav-tabs">
  <% if signed_in? %>
    <li class="nav-item">
      <%= link_to "Users", users_path %> 
    </li>
    <li class="nav-item">
      <%= link_to "Profile", current_user %>
    </li>
    <li class="nav-item">
      <%= link_to "Settings", edit_user_path(current_user) %>
    </li>
    <li class="nav-item">
      <%= link_to "Sign out", signout_path, method: "delete" %>
    </li>
  <% else %>
    <li><%= link_to "Sign in", signin_path %></li>
  <% end %>
</ul>